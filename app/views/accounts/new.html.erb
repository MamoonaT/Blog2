<br>
<%= form_for :account, url: student_accounts_path, method: :post do |f| %>
    <%= f.label :account_name %><br>
    <%= f.text_field :account_name %><br>
    <br>
    <%= f.label :account_number %><br>
    <%= f.text_field :account_number%><br>
    <br>
    <%= f.label :student_id %><br>
    <%= f.text_field :student_id, placeholder: "User ID" %><br>
    <%= f.submit %>
<% end %> 